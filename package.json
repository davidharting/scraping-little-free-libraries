{
  "version": "0.1.0",
  "license": "MIT",
  "main": "dist/index.js",
  "typings": "dist/index.d.ts",
  "files": [
    "dist",
    "src"
  ],
  "engines": {
    "node": ">=10"
  },
  "scripts": {
    "start": "tsdx watch",
    "build": "tsdx build",
    "run": "esr --cache ./src/index.ts",
    "test": "tsdx test",
    "lint": "tsdx lint",
    "prepare": "tsdx build",
    "size": "size-limit",
    "analyze": "size-limit --why"
  },
  "husky": {
    "hooks": {
      "pre-commit": "tsdx lint"
    }
  },
  "prettier": {
    "printWidth": 80,
    "semi": true,
    "singleQuote": true,
    "trailingComma": "es5"
  },
  "name": "little-free-libraries",
  "author": "David Harting",
  "module": "dist/little-free-libraries.esm.js",
  "size-limit": [
    {
      "path": "dist/little-free-libraries.cjs.production.min.js",
      "limit": "10 KB"
    },
    {
      "path": "dist/little-free-libraries.esm.js",
      "limit": "10 KB"
    }
  ],
  "devDependencies": {
    "@size-limit/preset-small-lib": "^7.0.8",
    "@types/papaparse": "^5.3.2",
    "husky": "^8.0.1",
    "pino-pretty": "^8.0.0",
    "size-limit": "^7.0.8",
    "tsdx": "^0.14.1",
    "tslib": "^2.4.0",
    "typescript": "^3.9.10"
  },
  "dependencies": {
    "axios": "^0.27.2",
    "esbuild-runner": "^2.2.1",
    "papaparse": "^5.3.2",
    "pino": "^8.0.0",
    "zod": "^3.17.3"
  }
}
